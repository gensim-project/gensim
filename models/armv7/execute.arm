execute(movt)
{
	uint32 orig = read_register_bank(RB, inst.rd);
	orig &= (uint32)0xffff;

	uint32 rn = (uint32)inst.rn;
	rn = rn << (uint32)12;

	uint32 imm16 = (uint32)inst.imm32 | (rn);

	orig |= (imm16) << (uint32)16;
	write_register_bank(RB, inst.rd, orig);
}

execute(movw)
{
	uint32 rn = (uint32)inst.rn;
	rn = rn << (uint32)12;
	uint16 imm16 = (uint16)((uint32)inst.imm32 | (rn));
