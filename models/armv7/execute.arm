////	BBL instructions
////////////////////////////////////////////////////////

execute(b)
{
	uint32 pc_var = read_pc();
	//uint32 imm32 = (signed_bits(inst.imm32, 23, 0) << 2) + 8;

	pc_var += inst.imm32;
	pc_var &= 0xfffffffe; //which will be this instruction

	write_register_bank(RB,15,pc_var);
}

execute(bl)
{
	uint32 pc_var = read_pc();
	//uint32 imm32 = (signed_bits(inst.imm32, 23, 0) << 2) + 8;
	
	write_register_bank(RB, 14, pc_var + 4);
	
	pc_var += inst.imm32;
	pc_var &= 0xfffffffe;
	
	write_register_bank(RB, 15, pc_var);
}

