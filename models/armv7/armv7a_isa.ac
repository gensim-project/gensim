		""   = 14;
	}

	ac_asm_map sf 
	{
		"" = 0;
		"s" = 1;    
	}

	ac_asm_map SHIFTTYPE
	{
		"lsl" = 0;
		"lsr" = 1;
		"asr" = 2;
		"ror" = 3;
	}
	
	ac_asm_map msrfieldmask
	{
		"" = 0;
		"c" =1 ;
		"x" = 2;
		"cx" = 3;
		"s" = 4;
		"sc" = 5;
		"sx" = 6;
		"scx" = 7;
		"f" = 8;
		"fc" = 9;
		"fx" = 10;
		"fcx" = 11;
		"fs" = 12;
		"fsc" = 13;
		"fsx" = 14;
		"fscx" = 15;
	}
	
	ac_asm_map memmultp
	{
		"a" = 0;
		"b" = 1;
	}
	
	ac_asm_map memmultu
	{
		"d" = 0;
		"i" = 1;
	}
	
	/*** Start of inst variables ***/	
	

