		"sp" = 13;
		"lr" = 14;
		"pc" = 15;
	}

	ac_asm_map cond 
	{
		"eq" = 0;
		"ne" = 1;
		"cs"= 2;
		"cc" = 3;
		"mi" = 4; 
		"pl" = 5;
		"vs" = 6;
		"vc" = 7;
		"hi" = 8;
		"ls" = 9;
		"ge" = 10;
		"lt" = 11;
		"gt" = 12;
