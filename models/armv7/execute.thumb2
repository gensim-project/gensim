helper uint32 zero_extend(uint32 imm4, uint32 i, uint32 imm3, uint32 imm8)
{
  uint32 result = 0;
  imm4 = imm4 << 12;
  i = i << 11;
  imm3 = imm3 << 8;
  result = imm4 | i | imm3 | imm8;
  return result;
}

