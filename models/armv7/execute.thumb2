helper void LoadWritePC(uint32 address)
{
	if (bitsel(address,0) == 1)
	{
		set_cpu_mode(1);
		write_register(T, 1);
		write_register_bank(RB, 15, address & ~1);
	}
	else if (bitsel(address,1) == 0)
	{
		set_cpu_mode(0);
		write_register(T, 0);
		write_register_bank(RB, 15, address);
	}
	else
	{
		trap();
	}
	return;
}

